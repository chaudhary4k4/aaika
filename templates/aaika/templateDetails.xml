  <config>
    <fields name="params">
      <fieldset name="LOGOOPTIONS">
        <field label="LOGOINFO"  type="spacer" class="alert alert-blue"/>
        <field name="logoFile" class="" type="media" default=""
					label="LOGO"
					description="Add Logo" />
      </fieldset>
      <fieldset name="COLOROPTIONS">
        <field label="COLORINFO"  type="spacer" class="alert alert-blue"/>
        <field name="themecolor" type="list" default="blu" label="SELECT THEME COLOR CODE" description="Select Theme Color" >
          <option value="default">Default</option>
          <option value="blue">blue</option>
          <option value="bridge">bridge</option>
          <option value="cyan">cyan</option>
          <option value="darkred">darkred</option>
          <option value="green">green</option>
          <option value="lightblue">lightblue</option>
          <option value="orange">orange</option>
          <option value="pink">pink</option>
          <option value="purple">purple</option>
          <option value="slate">slate</option>
          <option value="yellow">yellow</option>
        </field>
      </fieldset>
      <fieldset name="HOME_VERSION">
        <field label="HOME VERSION_INFO" type="spacer" class="alert-blue"/>
        <field name="home" type="list" default="home" label="Choose your Home" description="Choose your Layout">
          <option value="home_default">Home Defaut</option>
          <option value="home1">Home Layout 1</option>
          <option value="home2">Home Layout 2</option>
          <option value="home3">Home Layout 3</option>
          <option value="home4">Home Layout 4</option>
          <option value="home5">Home Layout 5</option>
          <option value="home6">Home Layout 6</option>
          <option value="home7">Home Layout 7</option>
          <option value="home8">Home Layout 8</option>
          <option value="home9">Home Layout 9</option>
          <option value="home10">Home Layout 10</option>
          <option value="home11">Home Layout 11</option>
          <option value="home12">Home Layout 12</option>
          <option value="home13">Home Layout 13</option>
          <option value="home14">Home Layout 14</option>
          <option value="home15">Text Slider</option>
          <option value="home16">Header with Portfolio</option>
          <option value="home17">Slider with Form</option>
          <option value="home18">Form in Header</option>
          <option value="home19">Home Layout 19</option>
          <option value="home20">Home Layout 20</option>
          <option value="home21">Home Layout 21</option>
          <option value="home22">Home Layout 22</option>
          <option value="home23">Home Layout 23</option>
          <option value="home24">Home Layout 24</option>
          <option value="home25">Home Layout 25</option>
          <option value="onepage">One Page</option>
          <option value="comingsoon">Coming Soon</option>
        </field>
      </fieldset>
      <fieldset name="LAYOUTOPTIONS">
        <field label="LAYOUTINFO"  type="spacer" class="alert alert-blue"/>
        <field name="layout" type="list" default="full" label="LAYOUT" description="Select Full Layout or Boxed Layout" >
          <option value="full">Full Layout</option>
          <option value="boxed">Boxed Layout</option>
        </field>
      </fieldset>
      <fieldset name="PATTERNOPTIONS">
        <field label="PATTERNINFO"  type="spacer" class="alert alert-blue"/>
        <field name="pattern" type="list" default="default" label="PATTERN" description="Select Pattern for Boxed Layout" >
          <option value="default">Pattern Default</option>
          <option value="1">Pattern 1</option>
          <option value="2">Pattern 2</option>
          <option value="3">Pattern 3</option>
          <option value="4">Pattern 4</option>
          <option value="5">Pattern 5</option>
          <option value="6">Pattern 6</option>
          <option value="7">Pattern 7</option>
          <option value="8">Pattern 8</option>
          <option value="9">Pattern 9</option>
          <option value="10">Pattern 10</option>
          <option value="11">Pattern 11</option>
        </field>
      </fieldset>
	  <fieldset name="CONTACT_DETAIL">
        <field label="CONTACT_US_INFO"  type="spacer" class="alert-blue"/>
        <field name="con_line1" type="text" label="Heading Line1" description="Enter the Heading line 1"  size="70"/>
        <field name="con_line2" type="text" label="Heading Line2" description="Enter the Heading line 2"  size="70"/>
        <field name="con_line3" type="text" label="Heading Line3" description="Enter the Heading line 3"  size="70"/>
      </fieldset>
      <fieldset name="SOCIALOPTIONS">
        <field label="SOCIALINFO"  type="spacer" class="alert-blue"/>
        <field name="social" type="radio" label="SHOWSOCIAL" description="Enable / Disable social icon" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="facebook" type="radio" label="Show Facebook" description="Facebook" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="facebooklink" type="text" label="Facebook Url" description="Facebook link" size="50" />
        <field name="twitter" type="radio" label="Show Twitter" description="Twitter" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="twitterlink" type="text" label="Twitter Url" description="Twitter link" size="50" />
        <field name="googleplus" type="radio" label="Show Google Plus" description="Google" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="googlepluslink" type="text" label="Google Plus Url" description="Google plus link" size="50" />
        <field name="linkedin" type="radio" label="Show LinkedIn" description="Link of Delicious" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="linkedinlink" type="text" label="LinkedIn Url" description="Linkedin link" size="50" />
        <field name="behance" type="radio" label="Show behance" description="behance" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="behancelink" type="text" label="Behance Url" description="behance" size="50" />
        <field name="flicker" type="radio" label="Show Flickr" description="Flicker" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="flickerlink" type="text" label="Flickr Url" description="Flicker Id" size="50" />
        <field name="youtube" type="radio" label="Show Youtube" description="Link of youtube" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="youtubelink" type="text" label="Youtube Url" description="Link of Youtube Channel" size="50" />
        <field name="dribbble" type="radio" label="Show Dribble" description="Dribble" default="1" checked="checked" class="btn-group">
          <option value="0">JNO</option>
          <option value="1">JYES</option>
        </field>
        <field name="dribbblelink" type="text" label="Dribble" description="Link of Dribble" size="50" />
      </fieldset>
      <fieldset name="TITLES">
        <field label="TITLE_INFO"  type="spacer" class="alert alert-blue"/>
        <field name="opblogtitle" type="text" label="One Page Blog title" description="One Page Blog Title" />
        <field name="opblogdesc" type="textarea" rows="2" cols="30" filter="raw" label="One Page Blog Description" description="One Page Blog Description" />
		<field name="cstitle" type="text" label="Coming Soon title" description="Coming Soon Title" />
        <field name="csdesc" type="textarea" rows="2" cols="30" filter="raw" label="Coming Soon Description" description="Coming Soon Description" />
		<field name="ctitle1" type="text" label="Contact title 1 for slider with form" description="Contact title 1" />
		<field name="ctitle2" type="text" label="Contact title 2 for slider with form" description="Contact title 2" />
      </fieldset>
    </fields>
  </config>
